{
  "manifest_version": 3,
  "name": "CRXJS React Vite Example",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "browsingData",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "./assets/index-mLWRVjGA.js"
      ]
    }
  ],
  "background": {
    "service_worker": "./assets/index-mLWRVjGA.js"
  }
}
